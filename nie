    Name = "[] : Midnight Blade"
})
local DragonTrident = Sword:AddLabel({
    Name = "[] : Dragon Trident"
})
local Yama = Sword:AddLabel({
    Name = "[] : Yama"
})
local BuddySword = Sword:AddLabel({
    Name = "[] : Buddy Sword"
})
local Canvander = Sword:AddLabel({
    Name = "[] : Canvander"
})
